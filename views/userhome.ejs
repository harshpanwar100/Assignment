      <h1>Dashboard</h1>
      
        
<br>
<!--table -->
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Car Name</th>
        <th scope="col">Manufacture Year</th>
        <th scope="col">Price</th>
      </tr>
    </thead>
    <tbody>
      <% let counter = 1; %>
      <%x.forEach((y)=>{%>
      <tr>
        <th scope="row"><%=counter%></th>
        <td><%=y.carName%></td>
        <td><%=new Date(y.manufYear).getFullYear()%></td>
        <td>â‚¹<%=y.price%></td>
      </tr>
      <% counter++; %>
      <%})%>
    </tbody>
  </table>
</body>
